+// CHECK:   store i8* %argp.next, i8** %va, align 8
+// CHECK:   %0 = bitcast i8* %argp.cur to i32*
+// CHECK:   %1 = load i32, i32* %0, align 8
+// CHECK:   store i32 %1, i32* %v, align 4
