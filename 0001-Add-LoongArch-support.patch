+// CHECK:   call void @llvm.va_end(i8* %va2)
+// CHECK:   %2 = load i32, i32* %v, align 4
+// CHECK:   ret i32 %2
+// CHECK: }
+int f_va_int(char *fmt, ...) {
+  __builtin_va_list va;
+  __builtin_va_start(va, fmt);
